<!DOCTYPE html>
<html>
  <head>
    <title>Radioconnection</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%  if member_signed_in? %>
     <li>
       <%= link_to "Member_Logout", destroy_member_session_path, method: :delete %>
     </li>
    <% else %>
     <li>
       <%= link_to "Member_Sign_up", new_member_registration_path %>
     </li>
     <Li>
       <%= link_to "Member_Login", new_member_session_path %>
     </Li>
    <% end %>
    <%= yield %>
  </body>
</html>
